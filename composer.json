{
    "name": "fifthgate/objectivity-calendar-generator",
    "description": "A simple calendar generation services",
    "homepage": "https://github.com/fifthgate/objectivity-calendar-generator",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Baynham",
            "email": "sam@fifthgate.net"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.2",
        "nesbot/carbon": "^2.43",
        "fifthgate/objectivity-core": "~2.1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.21",
        "orchestra/testbench-browser-kit": "^8.5",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Fifthgate\\Objectivity\\CalendarGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fifthgate\\Objectivity\\CalendarGenerator\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fifthgate\\Objectivity\\CalendarGenerator\\CalendarGeneratorServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/fifthgate/objectivity-core"
        }
    ]
}
